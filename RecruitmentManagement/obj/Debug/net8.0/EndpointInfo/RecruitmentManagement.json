  "components": {
    "schemas": {
      "Candidate": {
        "required": [
          "email",
          "experience",
          "jobId",
          "name",
          "skills"
        ],
        "type": "object",
        "properties": {
          "candidateId": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "minLength": 1,
            "type": "string"
          },
          "email": {
            "minLength": 1,
            "type": "string",
            "format": "email"
          },
          "phone": {
            "type": "string",
            "nullable": true
          },
          "experience": {
            "type": "integer",
            "format": "int32"
          },
          "college": {
            "type": "string",
            "nullable": true
          },
          "skills": {
            "minLength": 1,
            "type": "string"
          },
          "resumeFilePath": {
            "type": "string",
            "nullable": true
          },
          "jobId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Evaluator": {
        "type": "object",
        "properties": {
          "evaluatorId": {
            "type": "integer",
            "format": "int32"
          },
          "name": {
            "type": "string",
            "nullable": true
          },
          "email": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          },
          "designation": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Interview": {
        "required": [
          "candidateId",
          "date",
          "recruiterId"
        ],
        "type": "object",
        "properties": {
          "interviewId": {
            "type": "integer",
            "format": "int32"
          },
          "candidateId": {
            "type": "integer",
            "format": "int32"
          },
          "candidate": {
            "$ref": "#/components/schemas/Candidate"
          },
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "string",
            "nullable": true
          },
          "recruiterId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "Job": {
        "required": [
          "jobName",
          "type",
          "workMode"
        ],
        "type": "object",
        "properties": {
          "jobId": {
            "type": "integer",
            "format": "int32"
          },
          "jobName": {
            "minLength": 1,
            "type": "string"
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "type": {
            "minLength": 1,
            "type": "string"
          },
          "workMode": {
            "minLength": 1,
            "type": "string"
          },
          "candidates": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Candidate"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "WeatherForecast": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date"
          },
          "temperatureC": {
            "type": "integer",
            "format": "int32"
          },
          "temperatureF": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "summary": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
